{
  "name": "api-forms",
  "version": "1.0.0",
  "description": "form-builder-app",
  "engines": {
    "node": "13.9.0",
    "npm": "6.13.7"
  },
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -kill-others-on-fail \"npm run server\" \"npm run client\"",
