    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.3",
    "path": "^0.12.7",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-use-auth": "^0.5.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
